{
  "name": "jest-mock-vscode",
  "version": "1.0.2",
  "description": "VS Code Mock for jest",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "packageManager": "pnpm@7.9.0",
  "engines": {
    "node": ">16.0.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "pnpm run lint:eslint --fix && pnpm run lint:prettier",
    "lint:ci": "pnpm run lint:eslint && pnpm run lint:prettier-ci",
    "lint:eslint": "eslint \"src/**/*.ts\" \"*.js\"",
    "lint:prettier": "prettier -w \"**/*.{yaml,yml,md}\"",
    "lint:prettier-ci": "prettier -c \"**/*.{yaml,yml,md}\"",
    "prepare": "pnpm run build",
    "build-production": "pnpm run build",
    "build": "tsc -p .",
    "update-vscode": "pnpm up @types/vscode --latest",
    "watch": "pnpm run build --watch"
  },
  "keywords": [
    "jest",
    "vscode",
    "mock"
  ],
  "files": [
    "*.js",
    "*.d.ts",
    "dist",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/jest-mock-vscode.git"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rfdc": "^1.3.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "jest": "^28.1.3"
  },
  "dependencies": {
    "vscode-uri": "^3.0.3"
  }
}
