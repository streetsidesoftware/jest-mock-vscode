{
  "name": "jest-mock-vscode",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "version": "4.9.4",
  "description": "VS Code Mock for jest",
  "type": "commonjs",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd",
  "engines": {
    "node": ">22.0.0"
  },
  "scripts": {
    "test": "pnpm test:root && pnpm -r run test",
    "test:root": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist coverage temp",
    "lint": "pnpm run lint:eslint --fix && pnpm run lint:prettier",
    "lint:ci": "pnpm run lint:eslint && pnpm run lint:prettier-ci",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier -w .",
    "lint:prettier-ci": "prettier -c \"**/*.{yaml,yml,md}\"",
    "lint:spelling": "cspell --dot \"**\"",
    "prepublishOnly": "pnpm run build",
    "build-production": "pnpm run build",
    "build": "tsc -b . -f",
    "build:readme": "inject-markdown README.md && pnpm lint:prettier",
    "update-vscode": "pnpm up @types/vscode --latest",
    "watch": "pnpm run build --watch"
  },
  "keywords": [
    "jest",
    "vitest",
    "vscode",
    "mock"
  ],
  "files": [
    "*.js",
    "*.d.ts",
    "dist",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/jest-mock-vscode.git"
  },
  "pnpm": {
    "overrides": {},
    "patchedDependencies": {},
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@jest/globals": "^30.2.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^22.19.1",
    "@types/vscode": "^1.106.1",
    "eslint": "^9.39.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "inject-markdown": "^4.0.0",
    "jest": "^30.2.0",
    "prettier": "^3.7.1",
    "rfdc": "^1.4.1",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0"
  },
  "dependencies": {
    "vscode-uri": "^3.1.0"
  },
  "peerDependencies": {
    "@types/vscode": "^1.90.0"
  }
}
