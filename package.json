{
  "name": "jest-mock-vscode",
  "version": "1.0.0",
  "description": "VS Code Mock for jest",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "author": "Jason Dent",
  "license": "MIT",
  "packageManager": "pnpm@7.9.0",
  "scripts": {
    "test": "jest",
    "lint": "echo Lint",
    "prepare": "pnpm run build",
    "build-production": "pnpm run build",
    "build": "tsc -p .",
    "update-vscode": "pnpm up @types/vscode --latest",
    "watch": "pnpm run build --watch"
  },
  "keywords": [
    "jest",
    "vscode",
    "mock"
  ],
  "files": [
    "*.js",
    "*.d.ts",
    "dist",
    "!**/__mocks__",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streetsidesoftware/jest-mock-vscode.git"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.1",
    "jest": "^28.1.3",
    "rfdc": "^1.3.0",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "jest": "^28.1.3"
  },
  "dependencies": {
    "vscode-uri": "^3.0.3"
  }
}
